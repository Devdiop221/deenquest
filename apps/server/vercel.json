{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/bun"
    }
  ],
  "routes": [
    {
      "src": "/trpc/(.*)",
      "dest": "/src/index.ts"
    },
    {
      "src": "/api/auth/(.*)",
      "dest": "/src/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "src/index.ts": {
      "runtime": "bun"
    }
  }
}